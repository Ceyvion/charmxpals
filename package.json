{
  "name": "charmxpals",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:webpack": "next dev --no-turbo",
    "dev:safe": "NEXT_PUBLIC_DISABLE_3D=1 next dev --no-turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:up": "docker compose up -d db",
    "db:down": "docker compose down",
    "db:logs": "docker compose logs -f db",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:studio": "npx prisma studio",
    "seed": "npx ts-node --project tsconfig.seed.json prisma/seed.ts",
    "orchestrator": "node scripts/orchestrator.mjs",
    "e2e": "playwright test"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "framer-motion": "^10.12.16",
    "@react-three/drei": "^9.114.7",
    "@react-three/fiber": "^8.16.2",
    "next": "14.2.15",
    "react": "^18",
    "react-dom": "^18",
    "three": "^0.165.0",
    "jsqr": "^1.4.0",
    "animejs": "^3.2.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "postcss": "^8.5.6",
    "prisma": "^5.0.0",
    "tailwindcss": "^4.1.12",
    "ts-node": "^10.9.1",
    "typescript": "^5",
    "vitest": "^1.6.0",
    "@playwright/test": "^1.46.0"
  }
}
